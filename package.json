  "scripts": {
    "build": "npm run build:web && npm run build:server",
    "build:server": "npm run lint:server && npm run compile:server",
    "build:web": "npm run lint:web && npm run compile:web:prod",
    "compile:server": "cd server/ && webpack --config webpack.config.ts",
    "compile:web:dev": "cd web/ && webpack --config webpack.dev.config.ts",
    "compile:web:prod": "cd web/ && webpack --config webpack.prod.config.ts",
    "lint": "npm run lint:server && npm run lint:web",
    "lint:server": "eslint server/**/*.ts",
    "lint:web": "eslint web/**/*.ts",
    "prettify": "npm run prettify:server && npm run prettify:web",
    "prettify:server": "eslint --fix server/**/*.ts",
    "prettify:web": "eslint --fix web/**/*.ts",
    "serve": "nodemon --watch server/js --watch web/dist --watch config.json --delay 2500ms server/js/server.js",
    "start": "npm run lint && npm-run-all --race --parallel watch serve",
    "watch": "npm-run-all --race --parallel watch:server watch:web",
    "watch:server": "npm run compile:server -- --watch",
    "watch:web": "npm run compile:web:dev -- --watch"
  }
